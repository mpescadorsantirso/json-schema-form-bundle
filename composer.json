{
    "name": "mpescadorsantirso/json-schema-form-bundle",
    "description": "Create Symfony forms from JSON schema",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cyril Vmd",
            "homepage": "https://cyve.github.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "justinrainbow/json-schema": "^6.0",
        "symfony/form": "^6.0|^7.0",
        "symfony/validator": "^6.0|^7.0",
        "symfony/options-resolver": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "autoload": {
        "psr-4": {
            "Cyve\\JsonSchemaFormBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cyve\\JsonSchemaFormBundle\\Test\\": "tests"
        }
    }
}
